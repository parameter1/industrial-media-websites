{
  "taglib-imports": [
    "./load-more/marko.json"
  ],
  "<global-content-body-with-tracking>": {
    "template": "./content-body-with-tracking.marko",
    "@content": {
      "type": "object",
      "required": true
    },
    "@block-name": {
      "type": "string",
      "required": true
    }
  },
  "<global-section-cards-block>": {
    "template": "./section-cards.marko",
    "<query-params>": {},
    "<node>": {},
    "@title": "string",
    "@alias": "string",
    "@aliases": "array",
    "@ad-index": "array",
    "@ad-name": "string",
    "@ad-position": "string",
    "@count-only": "boolean"
  },
  "<global-content-meter-block>": {
    "template": "./content-meter.marko"
  }
}
